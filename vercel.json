{
  "version": 2,
  "projects": [
    {
      "name": "elib-dashboard",
      "rootDirectory": "elib-dashboard",
      "buildCommand": "npm run build",
      "outputDirectory": "dist"
    },
    {
      "name": "elib",
      "rootDirectory": "elib",
      "builds": [
        {
          "src": "server.ts",
          "use": "@vercel/node"
        }
      ],
      "routes": [
        {
          "src": "/api/(.*)",
          "dest": "src/server.ts"
        }
      ]
    }
  ]
}
